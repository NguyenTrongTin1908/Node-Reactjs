  <section class="featured spad">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="section-title">
            <h2>Featured Product</h2>
          </div>
          <div class="featured__controls">
            <ul>
              <li class="active" data-filter="*">All</li>
              <li data-filter=".thit">Fresh Meat</li>
              <li data-filter=".raucu">Vegetables</li>
              <li data-filter=".traicay">Fresh Fruits</li>
              <li data-filter=".haisan">Ocean Foods</li>
              <li data-filter=".doanlien">Fastfood</li>
              <li data-filter=".nuoc">Drinks</li>
              <li data-filter=".sua">Milk</li>
              <li data-filter=".banhkeo">Candy</li>
            </ul>
          </div>
        </div>
      </div>

      <div class="row featured__filter">
       
        <div class="col-lg-3 col-md-4 col-sm-6 mix ">
          <div class="featured__item">
