  <!-- Header Section End -->

  <!-- Hero Section Begin -->
  <%- include('selection'); %>
  <!-- Hero Section End -->
  
  <div class="w3-container">
  <div id="id01" class="w3-modal" style="display: none;">
    <div class="w3-modal-content" style="width: 400px;">
      <div class="modal-content ">
        <div class="modal-header">
          <h5 style="font-family: Cairo", sans-serif;">OGANI</h5>
          <button onclick="document.getElementById('id01').style.display='none'" type="button" class="close"
            data-dismiss="modal" style="font-size: 20px;">
            &times;
          </button>
        </div>
        <div class="modal-body">
          <p class="textsub"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal"
            onclick="document.getElementById('id01').style.display='none'" style="font-size: 15px;">
            Close
          </button>
        </div>
      </div>
    </div>
  </div>
  </div>

    <!-- Categories Section Begin -->

    <section class="categories">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="section-title">
              <h2>Sale off</h2>
            </div>
          </div>
          <div class="categories__slider owl-carousel">
           

  <div class="col-lg-3">
